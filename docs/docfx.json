{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/SampleDocApi/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/SampleFunctionsApp/**.csproj"
          ]
        }
      ],
      "dest": "functions",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md",
          "*.yml",
          "articles/**.md",
          "articles/**.yml",
          "articles/**/toc.yml",
          "functions/**",
          "restapi/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "contributors.json"
        ]
      },
      {
        "files": [
          "templates/cloud-awesome.docfx.liquid-base/images/**"
        ]
      },
      {
        "files": [
          "templates/cloud-awesome.docfx.liquid-base/styles/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [ "contributors.json" ],
    "fileMetadataFiles": [],
    "template": [
      "templates/cloud-awesome.docfx.liquid-base"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  },
  "pdf": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**.yml",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "*.yml",
          "restapi/**"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "*.md"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "templates/cloud-awesome.docfx.liquid-base.pdf/styles/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_pdf/**"
        ]
      }
    ],
    "wkhtmltopdf": {
      "additionalArguments": "--enable-local-file-access"
    },
    "dest": "_pdf",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "templates/cloud-awesome.docfx.liquid-base.pdf"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}