<h1 id="{{id}}" data-uid="{{uid}}" class="text-break">{% include partials/title %}</h1>
<div class="markdown level0 summary">{{ summary }}</div>
<div class="markdown level0 conceptual">{{ conceptual }}</div>

{% if inClass %}
    <div class="inheritance">
        <h5>{{ __global.inheritance }}</h5>
        {% for inheritor in inheritance %}
            <div class="level{{ inheritor.index }}">{{ inheritor.specName[0].value }}</div>
        {% endfor %}    
        <div class="level{{ level }}"><span class="xref">{{ name[0].value }}</span></div>
        
        {% for derivedClass in derivedClasses %}
            <div class="level{{ derivedClass.index }}">{{ derivedClass.specName[0].value}}</div>
        {% endfor %}
    </div>
{% endif %}

{% if implements %}
    <div class="implements">
        <h5>{{__global.implements}}</h5>
        {% for implement in implements %}
            <div>{{ implement.specName[0].value }}</div>
        {% endfor %}
    </div>
{% endif %}


{% if inheritedMembers %}
    <div class="inheritedMembers">
        <h5>{{__global.inheritedMembers}}</h5>
        {% for member in inheritedMembers %}
            <div>
                <xref uid="{{member.uid}}" text="{{member.nameWithType[0].value}}" alt="{{member.fullName[0].value}}"/>
            </div>
        {% endfor %}
    </div>
{% endif %}

<h6><strong>{{__global.namespace}}</strong>: {{ namespace.specName[0].value }}</h6>
<h6><strong>{{__global.assembly}}</strong>: {{ assemblies[0] }}.dll</h6>
<h5 id="{{id}}_syntax">{{__global.syntax}}</h5>
<div class="codewrapper">
    <pre><code class="lang-{{_lang}} hljs">{{ syntax.content[0].value }}</code></pre>
</div>

{% if syntax.parameters %}
    <h5 class="parameters">{{__global.parameters}}</h5>
    <table class="table table-bordered table-striped table-condensed">
        <thead>
        <tr>
            <th>{{__global.type}}</th>
            <th>{{__global.name}}</th>
            <th>{{__global.description}}</th>
        </tr>
        </thead>
        <tbody>

        {% for param in syntax.parameters %}
            <tr>
                <td>{{ param.type.specName[0].value }}</td>
                <td><span class="parametername">{{ param.id }}</span></td>
                <td>{{ param.description }}</td>
            </tr>   
        {% endfor %}


        </tbody>
    </table>
{% endif %}

{% if syntax.return  %}
    <h5 class="returns">{{__global.returns}}</h5>
    <table class="table table-bordered table-striped table-condensed">
        <thead>
        <tr>
            <th>{{__global.type}}</th>
            <th>{{__global.description}}</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>{{ syntax.return.type.specName[0].value }}</td>
            <td>{{ syntax.return.description }}</td>
        </tr>
        </tbody>
    </table>
{% endif %}

{% if syntax.typeParameters %}
    <h5 class="typeParameters">{{__global.typeParameters}}</h5>
    <table class="table table-bordered table-striped table-condensed">
        <thead>
            <tr>
                <th>{{__global.name}}</th>
                <th>{{__global.description}}</th>
            </tr>
        </thead>
        <tbody>
            {% for param in syntax.typeParameters %}
                <tr>
                    <td><span class="parametername">{{ param.id }}</span></td>
                    <td>{{ param.description }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endif %}

{% if remarks %}
    <h5 id="{{id}}_remarks"><strong>{{__global.remarks}}</strong></h5>
    <div class="markdown level0 remarks">{{ remarks }}</div>
{% endif %}
        
{% if example.size > 0 %}
    <h5 id="{{id}}_examples"><strong>{{__global.examples}}</strong></h5>
    {% for e in example %}
        <p>{{ e }}</p>
    {% endfor %}
{% endif %}